#category("World Changes")
#name("No Rain")
#description("Marsh starts sunny as it usually is after getting the sword tree. Also Howl is dead")

!flag("No Rain")

on spawn {
    store(HowlsDen.RainLifted, true)
    store(swampStateGroup.nightCrawlerChaseStarted, true)
    store(swampStateGroup.nightCrawlerDefeated, true)
    store(MarshSpawn.HowlBurnt, true)
}
